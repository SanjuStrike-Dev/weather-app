{
  "name": "weather-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.292.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "depcheck": "^1.4.7",
    "npm-check": "^5.9.2",
    "eslint-plugin-unused-imports": "^2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "review": "npm run review:imports && npm run review:deps && npm run review:check",
    "review:imports": "npm run lint:fix",
    "review:deps": "npx depcheck",
    "review:check": "npx npm-check",
    "review:check-interactive": "npx npm-check --interactive",
    "clean:imports": "npm run lint:fix",
    "check:imports": "npm run lint",
    "check:deps": "npx depcheck --json",
    "lint:fix": "eslint src/ --fix",
    "lint": "eslint src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
